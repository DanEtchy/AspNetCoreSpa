{
  "name": "aspnetnetcore",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "aspnetcore",
    "entityframework core",
    "angular2",
    "webpack2",
    "typescript2",
    "bootstrap4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asadsahi/AspNetCoreSpa.git"
  },
  "author": {
    "name": "Asad Sahi",
    "email": "asadazmat@gmail.com"
  },
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "protractor": "protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage wwwroot/dist",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build:aot": "npm run clean:dist && npm run clean:aot && node node_modules/webpack/bin/webpack.js --config config/webpack.prod.js --env.prod  --progress --profile --bail",
    "build:prod": "npm run clean:dist && node node_modules/webpack/bin/webpack.js --config config/webpack.prod.js --env.prod",
    "deploy:azure": "powershell ./deploy.azure.ps1",
    "deploy:smarter": "powershell ./deploy.smarter.ps1",
    "deploy:iis": "powershell ./deploy.iis.ps1",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "lint": "npm run tslint \"Client/**/*.ts\"",
    "postinstall": "npm run webdriver:update",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm-run-all -p -r server:prod:ci e2e:debug",
    "e2e:debug": "protractor --elementExplorer",
    "test": "npm run lint && karma start",
    "ci": "npm test && npm run e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./Client/",
    "compodoc": "compodoc -p tsconfig.json",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
